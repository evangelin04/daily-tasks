try:
    weight=int(input("Enter weight:"))
    if weight<0:
        print("INVALID INPUT")
    elif weight==0:
        print("Time Estimated:0 minutes")
    elif weight<=2000:
        print("Time Estimated:25 minutes")
    elif weight<=4000:
        print("Time Estimated:35 minutes")
    elif weight<=7000:
        print("Time Estimated:45 minutes")
    else:
        print("OVERLOADED")
except:
    print("INVALID INPUT")
